{
    "manifest_version": 3,
    "name": "IRCTC Extension",
    "description": "Version 1.0",
    "version": "1.0",
    "action": {
        "default_icon": "test_icon.png",
        "default_title": "Click to open Menu",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "webNavigation",
        "tabs"
    ],
    "host_permissions": [
        "https://www.irctc.co.in/nget/",
        "https://www.bus.irctc.co.in/"
    ],
    "background": {
        "service_worker": "background.js"
    }
}